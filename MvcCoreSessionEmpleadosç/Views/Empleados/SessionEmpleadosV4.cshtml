@model List<Empleado>
<h1>Session Empleados</h1>

<p><a asp-controller="Empleados" asp-action="EmpleadosAlmacenadosV4">Volver a la lista de empleados</a></p>
<h1>@ViewData["MENSAJE"]</h1>
<table class="table">
    <thead>
        <tr>
            <th>Apellido</th>
            <th>Salario</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var empleado in Model)
        {
            <tr>
                <td>@empleado.apellido</td>
                <td><a asp-controller="Empleados" asp-action="SessionEmpleadosV4" asp-route-idempleado="@empleado.emp_no">Almacenar empleado</a></td>
            </tr>
        }
    </tbody>
